CREATE DEFINER=`root`@`localhost` PROCEDURE `add_novo_Produto`(vercod varchar(50),vnome varchar(50),vsabor varchar(30),vtamanho varchar(50),vembalagem varchar(50),vpreco float(10.2))
BEGIN



INSERT INTO tabela_de_produtos (CODIGO_DO_PRODUTO,NOME_DO_PRODUTO,SABOR,TAMANHO,EMBALAGEM,PRECO_DE_LISTA)
    VALUES (vercod,vnome,vsabor,vtamanho,vembalagem,vpreco);
END
